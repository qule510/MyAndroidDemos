<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.qule.study">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".test.LazyFragmentActivity">
        </activity>
        <activity android:name=".activity.ShowTextActivity">
        </activity>
        <activity android:name=".basic.activity.AnimationActivity">
        </activity>
        <activity android:name=".test.HandlerTestActivity">
        </activity>
        <activity android:name=".basic.activity.ToolBarActivity">
        </activity>
        <activity
            android:name=".basic.activity.EditTextActivity"
            android:windowSoftInputMode="adjustUnspecified">
        </activity>
        <activity android:name=".basic.activity.ButtonActivity">
        </activity>
        <activity android:name=".basic.activity.CheckBoxActivity">
        </activity>
        <activity android:name=".basic.activity.RadioGroupActivity">
        </activity>
        <activity android:name=".basic.activity.ProgressBarActivity">
        </activity>
        <activity android:name=".basic.activity.SeekBarActivity">
        </activity>
        <activity android:name=".basic.activity.RatingBarActivity">
        </activity>
        <activity android:name=".basic.activity.DateAndTimeActivity">
        </activity>
        <activity android:name=".basic.activity.DialogActivity">
        </activity>
    </application>

</manifest>